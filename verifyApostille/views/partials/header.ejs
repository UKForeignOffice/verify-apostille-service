<div id="skiplink-container" class="noPrint">
    <div>
        <% if (typeof skip_override != 'undefined' && skip_override) { %>
        <a href="#<%= skip_override.link%>" class="skiplink"><%= skip_override.message%></a>
        <%}else{%>
        <a href="#content" class="skiplink">Skip to main content</a>
        <%}%>
    </div>
</div>

